{
    "name": "avkluchko/government-bundle",
    "description": "Utilities for use with Russian government classifiers and dictionaries",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Aleksandr Klyuchko",
            "email": "avkluchko@yandex.ru"
        }
    ],
    "require": {
        "php": "^7.1.3 || ^8.0",
        "symfony/config": "^4.0 || ^5.1.5",
        "symfony/dependency-injection": "^4.1.12 || ^5.1.5",
        "symfony/http-kernel": "^4.0 || ^5.1.5"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^4.1.12 || ^5.1.5",
        "squizlabs/php_codesniffer": "^3.6",
        "phpstan/phpstan": "^1.4",
        "phpstan/phpstan-symfony": "^1.1",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "AVKluchko\\GovernmentBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AVKluchko\\GovernmentBundle\\Tests\\": "tests/"
        }
    }
}
